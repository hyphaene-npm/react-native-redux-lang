{
	"ignore": ["node_modules/**"],
	"env": {
		"dev": {
			"presets": [
				["babel-preset-env", {
					"modules": false
				}], "babel-preset-react"
			],
			"plugins": [
				["transform-object-rest-spread"],
				["external-helpers"]
			]
		},
		"test": {
			"presets": [
				["babel-preset-env", {
					"modules": "commonjs"
				}], "babel-preset-react"
			],
			"plugins": [
				["transform-object-rest-spread"],
				["external-helpers"]
			]
		}
	}
}
